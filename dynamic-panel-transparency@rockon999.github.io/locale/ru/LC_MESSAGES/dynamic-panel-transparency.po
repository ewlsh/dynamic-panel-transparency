#: prefs.js/preferences/main.js:111
msgid "default"
msgstr "по-умолчанию"

#: prefs.js/preferences/main.js:499 prefs.js/ui/prefs.ui:1328
msgid "App Tweaks"
msgstr "Расширенные настройки"

#: prefs.js/preferences/main.js:734
msgid "Add a Custom WM_CLASS"
msgstr "Добавить пользовательский WM_CLASS"

#: prefs.js/ui/prefs.ui:11
msgid "Classy transparency for your panel."
msgstr "Настройка прозрачности панели."

#: prefs.js/ui/prefs.ui:13
msgid "Website"
msgstr "Веб-сайт"

#: prefs.js/ui/prefs.ui:107
msgid "Shell Restart Required."
msgstr "Необходим перезапуск Shell"

#. Meant to ask the user if they would like to restart right now or later in a yes/no format.
#: prefs.js/ui/prefs.ui:108
msgid ""
"A shell restart is required to view some of your changes. Do you want to "
"restart now?"
msgstr ""
"Необходимо перезапустить Shell для применения изменений. Хотите перезапустить"
" сейчас?"

#. How long the transition will take.
#: prefs.js/ui/prefs.ui:188
msgid "Transition Speed"
msgstr "Скорость перехода"

#: prefs.js/ui/prefs.ui:218 prefs.js/ui/prefs.ui:246
msgid "Overrides 'gtk-enable-animations'."
msgstr "Переопределение 'gtk-enable-animations'."

#. Whether the panel transitions should happen with the overview transitions or after.
#: prefs.js/ui/prefs.ui:231
msgid "Transition with the overview"
msgstr "Переход в режиме Обзор"

#. Whether the panel should transition when a window touches it or only when a window is actually maximized.
#: prefs.js/ui/prefs.ui:259
msgid "Transition when windows touch the panel"
msgstr "Переход при косании панели окном"

#: prefs.js/ui/prefs.ui:276
msgid "Transitions"
msgstr "Скорость перехода"

#. Allows the user to set their own text color.
#: prefs.js/ui/prefs.ui:333
msgid "Enable custom text coloring"
msgstr "Настроить цвет текста"

#. The main color to be used.
#: prefs.js/ui/prefs.ui:360 prefs.js/ui/prefs.ui:375
msgid "Primary Color"
msgstr "Основной цвет"

#. Another color for special functions.
#: prefs.js/ui/prefs.ui:389 prefs.js/ui/prefs.ui:403
msgid "Secondary Color"
msgstr "Дополнительный цвет"

#: prefs.js/ui/prefs.ui:423
msgid "Use when a window is maximized"
msgstr "Использовать при развёрнутом окне"

#: prefs.js/ui/prefs.ui:446
msgid "Use when the overview is visible"
msgstr "Использовать в режиме Обзор"

#: prefs.js/ui/prefs.ui:514
msgid "Enable text shadowing"
msgstr "Добавить тень для текста"

#: prefs.js/ui/prefs.ui:554 prefs.js/ui/prefs.ui:615 prefs.js/ui/prefs.ui:753
#: prefs.js/ui/prefs.ui:814
msgid "Shadow Color"
msgstr "Цвет тени"

#: prefs.js/ui/prefs.ui:628 prefs.js/ui/prefs.ui:827
msgid "Radius"
msgstr "Радиус"

#: prefs.js/ui/prefs.ui:641 prefs.js/ui/prefs.ui:840
msgid "Horizontal Offset"
msgstr "По горизонтали"

#: prefs.js/ui/prefs.ui:654 prefs.js/ui/prefs.ui:853
msgid "Vertical Offset"
msgstr "По вертикали"

#: prefs.js/ui/prefs.ui:713
msgid "Enable icon shadowing"
msgstr "Добавить тень для значков"

#: prefs.js/ui/prefs.ui:899
msgid "Foreground"
msgstr "Текст и тени"

#. Opacity of the panel when no window is maximized.
#: prefs.js/ui/prefs.ui:961
msgid "Unmaximized Opacity"
msgstr "Минимальная прозрачность"

#. Opacity of the panel when a window is maximized.
#: prefs.js/ui/prefs.ui:1008 prefs.js/ui/app-prefs.ui:117
msgid "Maximized Opacity"
msgstr "Максимальная прозрачность"

#. Allows the user to set their own opacity.
#: prefs.js/ui/prefs.ui:1030
msgid "Enable custom opacity"
msgstr "Настроить прозрачность"

#. The panel's background color.
#: prefs.js/ui/prefs.ui:1097 prefs.js/ui/prefs.ui:1112
#: prefs.js/ui/app-prefs.ui:91
msgid "Panel Color"
msgstr "Цвет панели"

#. Allows the user to set their own background color for the panel.
#: prefs.js/ui/prefs.ui:1133
msgid "Enable custom panel color"
msgstr "Настроить цвет панели"

#. Removes any extra styling certain themes may have. Fixes theme issues.
#: prefs.js/ui/prefs.ui:1197
msgid "Remove excess panel styling"
msgstr "Удалить лишние стили"

#: prefs.js/ui/prefs.ui:1210
msgid "(Fixes theme incompatibilities.)"
msgstr "(Исправляет несовместимость с темой)"

#. Hides the panel corners.
#: prefs.js/ui/prefs.ui:1238
msgid "Hide corners"
msgstr "Скрыть углы"

#: prefs.js/ui/prefs.ui:1266
msgid "Background"
msgstr "Фон"

#: prefs.js/ui/prefs.ui:1368
msgid "About"
msgstr "О приложении"

#. Adds a 'custom' app tweak. Only visible on the App Tweaks page.
#: prefs.js/ui/prefs.ui:1396
msgid "Advanced..."
msgstr "Дополнительно..."

#: prefs.js/ui/prefs.ui:1432
msgid ""
"A WM_CLASS is a code that identifies a window. Use this setting if the "
"window you want custom settings for is not part of an application. You can "
"find a window's WM_CLASS with the command 'xprop WM_CLASS'. Enter the second "
"result from 'xprop WM_CLASS' below without quotation marks."
msgstr ""
"WM_CLASS - это код, который определяет окно. Используйте этот параметр, если "
"окно, которое необходимо настроить не является частью приложения. Получить "
"WM_CLASS для окна можно с помощью команды 'xprop WM_CLASS'. Введите второй "
"результат вывода 'xprop WM_CLASS' ниже без кавычек."

#: prefs.js/ui/prefs.ui:1481
msgid "WM_CLASS cannot be blank."
msgstr "WM_CLASS не может быть пустым"

#: prefs.js/ui/app-prefs.ui:51
msgid "Enable background app tweaks"
msgstr ""

#: prefs.js/ui/app-prefs.ui:130
msgid "Theme Source"
msgstr "Исходная тема"

#: prefs.js/ui/app-prefs.ui:190
msgid "Always trigger the panel."
msgstr ""

#~ msgid "Force animation"
#~ msgstr "Принудительная анимация"

#~ msgid "Transition Style"
#~ msgstr "Скорость перехода"

#~ msgid "Linear (default)"
#~ msgstr "по-умолчанию"
